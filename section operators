
public class Sectionoperators {

    public static void main(String[] args) {
        int a=10;
        int b=5;
        int c=a & b;
        System.out.println(c);
        int d=a|b;
        System.out.println(d);
        int e=a^b;
        System.out.println(e);
        int f=a>>3;
        System.out.println(f);
        int g=10<<3;
        System.out.println(g);
    }
}
